{
  "name": "univeral-hmr-ssr-react-redux",
  "version": "1.0.0",
  "description": "Univeral JS application utilizing Hot Module Reloading, Server Side Rendering, Code Splitting with React, Redux, React Router.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "eslint": "eslint .",
    "development": "NODE_PATH=./src NODE_ENV=development node ./src/server/server.babel.js",
    "prod": "npm run production",
    "production": "NODE_PATH=./src NODE_ENV=production node ./src/server/server.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:client": "NODE_ENV=production webpack --config ./webpack/client.babel.js"
  },
  "keywords": [
    "react",
    "redux",
    "hot-module-reloading",
    "server-side-rendering",
    "hmr",
    "ssr",
    "code-splitting",
    "webpack"
  ],
  "author": "Alexander J Ray <alexjamesray@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
